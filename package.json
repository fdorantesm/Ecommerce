    "consign": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "i18n-2": "^0.7.3",
    "jsonwebtoken": "^8.3.0",
